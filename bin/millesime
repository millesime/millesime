#!/usr/bin/env php
<?php

$file = '/vendor/autoload.php';

$requireif = function($path) use ($file) {
	return file_exists($path.$file) ? require $path.$file : false;
};

($loader = $requireif(__DIR__.'/..')) || ($loader = $requireif(__DIR__.'/../../..'));

$millesime = new Millesime\Application();
$millesime->run();
